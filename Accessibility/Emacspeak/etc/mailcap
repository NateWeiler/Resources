video/*; ${EMACSPEAK_DIR}/bash-utils/emplay %s
audio/*; ${EMACSPEAK_DIR}/bash-utils/emplay %s
application/pdf; pdftotext -layout %s -
application/atom+xml; xsltproc ${EMACSPEAK_DIR}/xsl/atom.xsl %s
application/rss+xml; xsltproc ${EMACSPEAK_DIR}/xsl/rss.xsl %s
application/xhtml+xml; xsltproc ${EMACSPEAK_DIR}/xsl/identity.xsl %s
